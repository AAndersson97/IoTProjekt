<AnchorPane prefHeight="200.0" prefWidth="178.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.SybilAttackUI">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Adress of node to attack">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="nodeAddrs" layoutX="10.0" layoutY="64.0" onMouseClicked="#choiceboxShown" prefWidth="150.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </ChoiceBox>
      <Button fx:id="attackBtn" layoutX="58.0"  disable="true" layoutY="150.0" mnemonicParsing="false" onAction="#onStartAttack" stylesheets="@Style.css" text="Attack" />
   </children>
</AnchorPane>
